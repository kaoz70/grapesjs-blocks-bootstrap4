{
  "name": "grapesjs-blocks-bootstrap4",
  "version": "0.2.4",
  "description": "GrapesJS Bootstrap v4 Blocks Plugin",
  "main": "dist/grapesjs-blocks-bootstrap4.min.js",
  "scripts": {
    "lint": "eslint src",
    "v:patch": "npm version --no-git-tag-version patch",
    "v:minor": "npm version --no-git-tag-version minor",
    "build": "npm run v:patch && webpack --env production",
    "build-dev": "npm run && webpack --env production",
    "start": "webpack-cli serve --mode development --progress"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaoz70/grapesjs-bootstrap4-blocks.git"
  },
  "keywords": [
    "grapesjs",
    "plugin",
    "bootstrap"
  ],
  "author": "z1lk",
  "license": "BSD-3-Clause",
  "peerDependencies": {
    "grapesjs": "0.x"
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.0",
    "@babel/preset-env": "^7.13.5",
    "@webpack-cli/init": "^1.1.3",
    "babel-loader": "^8.2.2",
    "eslint": "^7.20.0",
    "html-webpack-plugin": "^5.2.0",
    "raw-loader": "^4.0.2",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "npm": "^7.0.14"
  }
}
